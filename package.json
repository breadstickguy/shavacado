{
  "name": "shavacado",
  "version": "0.0.1",
  "description": "All things avocado",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/breadstickguy/shavacado.git"
  },
  "keywords": [
    "avocado",
    "shavacado",
    "shavocado"
  ],
  "author": "Andrew Mendez, David Barrios, Mike Mayo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/breadstickguy/shavacado/issues"
  },
  "homepage": "https://github.com/breadstickguy/shavacado#readme",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "if-env": "^1.0.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.2",
    "nodemon": "^2.0.2"
  }
}
